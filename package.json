    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "dev:ssr": "ng run gamer:serve-ssr",
    "serve:ssr": "node dist/apps/server/main.js",
    "build:ssr": "ng build --configuration production && ng run gamer:server:production",
